<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="stock.action_stock_scrap" model="ir.actions.act_window">
            <field name="name">Move to Unserviceable</field>
        </record>
        <record id="mrp.mrp_production_action" model="ir.actions.act_window">
            <field name="name">Prepacking Orders</field>
        </record>
        <record id="mrp.mrp_unbuild" model="ir.actions.act_window">
            <field name="name">Reverse Orders</field>
        </record>
        <record id="mrp.mrp_bom_form_action" model="ir.actions.act_window">
            <field name="name">Prepacking Templates</field>
            <field name="res_model">mrp.bom</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'default_type': 'normal'}</field>
        </record>
         <record id="stock.action_production_lot_form" model="ir.actions.act_window">
            <field name="name">Batches/Serial Numbers</field>
        </record>
        <record id="mrp.mrp_production_report" model="ir.actions.act_window">
            <field name="name">Prepacking Orders</field>
        </record>
         <!-- Disable BoM Overview print report -->
        <record id="mrp.action_report_bom_structure" model="ir.actions.report">
            <field name="binding_model_id" eval="False"/>
            <field name="groups_id" eval="[(6, 0, [ref('base.group_system')])]"/>
        </record>

    </data>
</odoo>